<!DOCTYPE html>
<html>
<head>
  <title>HuanTarget | <%= yield(:title) %></title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    <div id="container">
        <div id="header">
            <a href="" id="logo"><h1>HuanTarget</h1></a>
            <div id="menu">
                <ul>
                    <li>PROF BLOG</li>
                    <li>PROJECTS</li>
                    <li>FOR RECRUITERS</li>
                    <li>CONTACT ME</li>
                </ul>
            </div>
        </div>

        <div id="content">
            <%= yield %>
        </div>

        <div id="footer"><h1>Footer</h1></div>
    </div>
</body>
</html>
